<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="OFFMarketStartupWeekend.controller.CustomerMain" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>CustomerMainText}"
		        showNavButton="true"
		        navButtonPress="handleNavButtonPress">
				<content>
				    <List id="SectorList" mode="None" items="{Tasks>/taskList}" >
                        <StandardListItem xmlns="sap.m"
                            id="id"
                            type="Active"
                            visible="true"
                            press="handleListItemPress"
                            selected= "handleListItemPress"
                            title="{Tasks>sectorName}"
                            icon="sap-icon://basket"
                            />
                        </List>
				</content>
				<footer>
				    <OverflowToolbar>
				        <ToolbarSpacer/>
				        <Image
            		    	visible="true"
            		    	width="80px"
            		    	height="40px"
            		    	src="/webapp/Image/Image1.jpg"/>
				    </OverflowToolbar>
        		</footer>
			</Page>
		</pages>
	</App>
</mvc:View>